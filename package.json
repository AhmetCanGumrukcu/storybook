  "scripts": {
    "lint": "node .scripts/lint.js",
    "lintfix": "node .scripts/lint.js --fix",
    "storybook": "start-storybook -p 9010",
    "start": "concurrently  --kill-others \"start-storybook -p 9001\" \"node server\"",
    "fullbuild": "babel-node build/main.js --fullBuild --concurrentPromise=10",
